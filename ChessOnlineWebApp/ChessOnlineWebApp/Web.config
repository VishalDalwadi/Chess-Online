<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.7.2"/>
    <httpRuntime targetFramework="4.7.2"/>
  </system.web>
  <system.codedom>
    <compilers>
      <compiler language="c#;cs;csharp" extension=".cs"
        type="Microsoft.CodeDom.Providers.DotNetCompilerPlatform.CSharpCodeProvider, Microsoft.CodeDom.Providers.DotNetCompilerPlatform, Version=2.0.1.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"
        warningLevel="4" compilerOptions="/langversion:default /nowarn:1659;1699;1701"/>
      <compiler language="vb;vbs;visualbasic;vbscript" extension=".vb"
        type="Microsoft.CodeDom.Providers.DotNetCompilerPlatform.VBCodeProvider, Microsoft.CodeDom.Providers.DotNetCompilerPlatform, Version=2.0.1.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"
        warningLevel="4" compilerOptions="/langversion:default /nowarn:41008 /define:_MYTYPE=\&quot;Web\&quot; /optionInfer+"/>
    </compilers>
  </system.codedom>

  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IAuthorizationService" />
        <binding name="BasicHttpBinding_IUserProfileManagementService" />
        <binding name="BasicHttpBinding_IAuthenticationService" />
        <binding name="BasicHttpBinding_IGamesManagementService" />
        <binding name="BasicHttpBinding_IEmailService" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:8733/Design_Time_Addresses/AuthorizationService/AuthorizationService/"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAuthorizationService"
        contract="AuthorizationServiceReference.IAuthorizationService"
        name="BasicHttpBinding_IAuthorizationService" />
      <endpoint address="http://localhost:8733/Design_Time_Addresses/RegistrationService/RegistrationService/"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IUserProfileManagementService"
        contract="UserProfileServiceReference.IUserProfileManagementService"
        name="BasicHttpBinding_IUserProfileManagementService" />
      <endpoint address="http://localhost:8733/Design_Time_Addresses/AuthenticationService/AuthenticationService/"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAuthenticationService"
        contract="AuthenticationServiceReference.IAuthenticationService"
        name="BasicHttpBinding_IAuthenticationService" />
      <endpoint address="http://localhost:8733/Design_Time_Addresses/GamesManagementService/GamesManagementService/"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IGamesManagementService"
        contract="GamesManagementServiceReference.IGamesManagementService"
        name="BasicHttpBinding_IGamesManagementService" />
      <endpoint address="http://localhost:8733/Design_Time_Addresses/EmailService/EmailService/"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IEmailService"
        contract="EmailServiceReference.IEmailService" name="BasicHttpBinding_IEmailService" />
    </client>
  </system.serviceModel>
</configuration>
